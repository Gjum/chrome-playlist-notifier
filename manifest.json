{
  "manifest_version": 2,
  "name": "Inspect Client",
  "description": "Inspects tabs for data and sends it to a local web server.",
  "version": "0.0.2",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "19": "img/icon-19.png",
    "24": "img/icon-24.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
